{
  "version": 2,
  "builds": [
    {
      "src": "server/entry-server.js", 
      "use": "@vercel/node"
    },
    {
      "src": "dist/client/**", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)", 
      "dest": "dist/client/static/$1"
    },
    {
      "src": "/(.*)", 
      "dest": "server/entry-server.js"
    }
  ]
}